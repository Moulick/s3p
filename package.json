{
  "author": "GenUI LLC",
  "bin": {
    "s3p": "./s3p"
  },
  "bugs": "https:/github.com/generalui/s3p/issues",
  "dependencies": {
    "@art-suite/cli": "^1.3.3",
    "art-class-system": "^1.11.6",
    "art-standard-lib": "^1.69.4",
    "aws-sdk": "^2.643.0",
    "caffeine-script-runtime": "^1.13.3",
    "neptune-namespaces": "^4.0.0",
    "shell-escape": "^0.2.0"
  },
  "description": "S3P is a CLI and library that is 5x to 50x faster than aws-cli for bulk S3 operations including: summarize, compare, copy and sync.",
  "devDependencies": {
    "art-build-configurator": "^1.26.9",
    "art-testbench": "^2.0.0",
    "caffeine-script": "^0.72.1",
    "jest": "^26.4.2",
    "mocha": "^7.1.1",
    "mock-fs": "^4.10.0",
    "npm-watch": "^0.7.0"
  },
  "homepage": "https://github.com/generalui/s3p",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "ISC",
  "name": "s3p",
  "repository": {
    "type": "git",
    "url": "https://github.com/generalui/s3p.git"
  },
  "scripts": {
    "build": "caf -p -c source-source -o source\nnn -sq",
    "build-clean": "rm -rf source/*; npm run build",
    "start": "webpack-dev-server --hot --inline --progress --env.devServer",
    "test": "nn -s;mocha -u tdd",
    "testInBrowser": "webpack-dev-server --progress --env.devServer",
    "watch": "npm-watch"
  },
  "version": "2.7.0",
  "watch": {
    "build": {
      "extensions": "caf",
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
  }
}
