  "license": "ISC",
  "name": "s3p",
  "repository": {
    "type": "git",
    "url": "https://github.com/generalui/s3p.git"
  },
  "scripts": {
    "build": "caf -p -c source-source -o source\nnn -sq",
    "build-clean": "rm -rf source/*; npm run build",
    "test": "jest",
    "watch": "npm-watch"
  },
  "version": "2.9.2",
  "watch": {
    "build": {
      "extensions": "caf",
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
