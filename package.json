{
  "author": "GenUI LLC",
  "bin": {
    "s3p": "./bin/s3p"
  },
  "bugs": "https:/github.com/generalui/s3p/issues",
  "dependencies": {
    "@art-suite/cli": "^1.3.10",
    "art-class-system": "^1.11.12",
    "art-standard-lib": "^1.70.3",
    "aws-sdk": "^2.790.0",
    "caffeine-script-runtime": "^1.14.0",
    "neptune-namespaces-runtime": "^3.2.8",
    "shell-escape": "^0.2.0"
